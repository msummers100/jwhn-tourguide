<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tour_orlando">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ThemeParks"
            android:label="Theme Parks in Orlando" />
        <activity
            android:name=".Restaurants"
            android:label="Restaurants in Orlando" />
        <activity
            android:name=".Golf"
            android:label="Golf Resorts in Orlando" />
        <activity
            android:name=".ThingsToDo"
            android:label="Things To Do in Orlando"></activity>
    </application>

</manifest>